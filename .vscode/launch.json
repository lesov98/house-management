{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug NestJS",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run-script", "start:debug"],
      "port": 9229,
      "sourceMaps": true,
      "smartStep": true,
      "protocol": "inspector",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    }
  ]
}
